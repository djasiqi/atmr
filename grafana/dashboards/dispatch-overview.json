{
  "dashboard": {
    "title": "ATMR Dispatch Overview",
    "tags": ["dispatch", "atmr", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Dispatch Runs Total",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(dispatch_runs_total[5m])) by (status, mode)",
            "legendFormat": "{{status}} - {{mode}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Runs/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Dispatch Duration (Histogram)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(dispatch_duration_seconds_bucket[5m])) by (le, mode, company_id))",
            "legendFormat": "p95 - {{mode}} - company={{company_id}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(dispatch_duration_seconds_bucket[5m])) by (le, mode, company_id))",
            "legendFormat": "p50 - {{mode}} - company={{company_id}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Dispatch Quality Score",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "dispatch_quality_score",
            "legendFormat": "Company {{company_id}} - Run {{dispatch_run_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "none", "min": 0, "max": 100, "label": "Score (0-100)"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Assignment Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "dispatch_assignment_rate",
            "legendFormat": "Company {{company_id}} - Run {{dispatch_run_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100, "label": "Rate (%)"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Unassigned Bookings Count",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "dispatch_unassigned_count",
            "legendFormat": "Company {{company_id}} - Run {{dispatch_run_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Count"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Circuit Breaker State",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "dispatch_circuit_breaker_state",
            "legendFormat": "Company {{company_id}} (0=CLOSED, 1=OPEN, 2=HALF_OPEN)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "min": 0, "max": 2, "label": "State"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Temporal Conflicts",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(dispatch_temporal_conflicts_total[5m])) by (company_id)",
            "legendFormat": "Company {{company_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Conflicts/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "DB Conflicts",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(dispatch_db_conflicts_total[5m])) by (company_id)",
            "legendFormat": "Company {{company_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Conflicts/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "OSRM Cache Hit Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "osrm_cache_hit_rate",
            "legendFormat": "Hit Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "min": 0, "max": 1, "label": "Hit Rate"},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "title": "OSRM Cache Hits/Misses",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "sum(rate(osrm_cache_hits_total[5m])) by (cache_type)",
            "legendFormat": "Hits - {{cache_type}}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(osrm_cache_misses_total[5m])) by (cache_type)",
            "legendFormat": "Misses - {{cache_type}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Operations/sec"},
          {"format": "short"}
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {}
  }
}

